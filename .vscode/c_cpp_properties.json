{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "/usr/bin/gcc",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "includePath": [
                "${config:idf.espIdfPath}/components/**",
                "${config:idf.espIdfPathWin}/components/**",
                "${config:idf.espAdfPath}/components/**",
                "${config:idf.espAdfPathWin}/components/**",
                "${workspaceFolder}/**",
                "${workspaceFolder}/components/components/esp_driver_gpio/include",
                "${workspaceFolder}/main/custom_components/*",
                "${workspaceFolder}/components/components/xtensa/include",
                "${workspaceFolder}/main/custom_components/",
                "${workspaceFolder}/build/config/",
                "${workspaceFolder}/main/**",
                "/home/johannes/esp/esp-idf/components/esp_netif/linux/stubs/include/machine/*"
            ],
            "browse": {
                "path": [
                    "${config:idf.espIdfPath}/components",
                    "${config:idf.espIdfPathWin}/components",
                    "${config:idf.espAdfPath}/components/**",
                    "${config:idf.espAdfPathWin}/components/**",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
